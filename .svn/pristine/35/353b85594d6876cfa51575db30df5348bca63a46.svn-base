@charset "utf-8";
/* CSS Document */

body{margin:0px; padding:0px; font-family: 'Roboto', sans-serif;}
a:focus { outline:none; outline-offset: -2px;}
.nav-tabs{background: #000}
/*Header-section*/
.navbar-inverse {background-color:#ffb502; border-color: transparent; box-shadow:0 2px 8px rgba(0,0,0,0.4);}
.navbar-brand {height:inherit; display:block;/* padding:15px 0 15px 72px;*/}

.hours{font-size:45px !important; color:#ab2101 !important; font-weight:400;}
