.icon-color {color: rgb(255, 255, 255);margin-top: 14px;}

.affix .navbar-brand img {max-width:70%;}
.affix .ul-margin {margin:22px 0;}
.affix .navbar {min-height:inherit; }
.affix .navbar, .navbar-brand img, .ul-margin{
-webkit-transition:all 0.4s linear;
-moz-transition:all 0.4s linear;
-o-transition:all 0.4s linear;
transition:all 0.4s linear;
}

/*Navbar-section*/
.ul-margin {margin:40px 90px 20px 90px;}
.navbar-inverse .navbar-nav > .active > a, .navbar-inverse .navbar-nav > .active > a:hover, .navbar-inverse .navbar-nav > .active > a:focus {background-color:transparent; border:1px solid #e15d05; color: #e15d05;}
.navbar-inverse .navbar-nav > li > a {color:#000000; }
.nav > li > a {padding:8px 15px;}
.navbar-nav > li {margin-left:8px;}
.navbar-nav.navbar-right:last-child {}
.navbar-inverse .navbar-nav > li > a:hover, .navbar-inverse .navbar-nav > li > a:focus {color:#4C4A4A}
.login-sign {background:#c35207;color:#ffffff !important; font-weight:400 !important;}
.login-sign:hover {background:#4C4A4A !important}


/*icon-section*/
.secondmenu {width:100%; margin:127px 0 15px; text-align:center;}
.secondmenu ul { margin:0px; padding:0px;}
.secondmenu ul li { display:inline-block; font-size:22px; color:#e15d05;}
.span:hover{ color:#c35207;}

.secondmenu ul li a	{display: inline-block;	font-family: 'Roboto', sans-serif;text-decoration:none;}
.secondmenu ul li:hover a { color: #e15d05; }

.secondmenu ul li span a:hover  {color:#de302f;}

.sp-img, .sp-title { display: block; }
.secondmenu ul li a span {display:block; width:100%; color:#e15d05; text-transform:uppercase; font-weight:bold;}
a.span:hover{color: #000000; padding:0px;}
.secondmenu ul li .sp-img a { width: 107px; height: 107px; color:rgb(225, 93, 5); font-weight: bold; text-transform: uppercase; font-size: 22px; display: inline-block; }

.secondmenu ul li a {color: #ffb502;font-size: 16px;font-weight: 400;margin: 5px 0;text-transform: none;}

.rdBtns {padding-bottom: 30px;}

.sign-width {width:30% !important; float:left; margin-top:0 !important; margin-right:35px;}

