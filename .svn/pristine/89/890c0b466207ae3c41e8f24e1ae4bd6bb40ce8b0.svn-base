/** loading screen **/
.loader{
width: 300px;
	margin:auto;
	margin-top:100px;
}
.loading-spiner-holder{
     position: absolute;
  width: 100%;
  height: 100%;
  z-index: 1000;
  background: url('../img/loader_gif.gif') 50% 50% no-repeat rgb(249,249,249);
  opacity: 0.7;
}
.loading-image{
	position: absolute;
   top:50%;
   left:50%;
   margin-left:-100px;
}
  .typeahead-demo .custom-popup-wrapper {
	position: absolute;
	top: 100%;
	left: 0;
	z-index: 1000;
	display: none;
	background-color: #f9f9f9;
  }

  .typeahead-demo .custom-popup-wrapper > .message {
	padding: 10px 20px;
	border-bottom: 1px solid #ddd;
	color: #868686;
  }

  .typeahead-demo .custom-popup-wrapper > .dropdown-menu {
	position: static;
	float: none;
	display: block;
	min-width: 160px;
	background-color: transparent;
	border: none;
	border-radius: 0;
	box-shadow: none;
  }
.fl{
	float:left !important;
}
.fr{
	float: right !important;
}
.clear{
	clear: both !important;
}
.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover{
	color: #ffb502;
    background-color: #000;
    border: 1px solid #000;
}
.nav-tabs>li>a:hover{
	color: #fff;
	background-color: #000;
	border: 1px solid #000;
}
.image-100{
	width:100px;
	height:100px;
}
.image-400{
	width:400px;
	height:400px;
}
